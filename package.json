    "typescript",
    "angular"
  ],
  "author": "@uzenith360",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/uzenith360/ngx-navigation-animation/issues"
  },
  "homepage": "https://github.com/uzenith360/ngx-navigation-animation#readme",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/",
    "access": "public",
    "tag": "latest"
  },
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "@angular/animations": "^17.0.9",
